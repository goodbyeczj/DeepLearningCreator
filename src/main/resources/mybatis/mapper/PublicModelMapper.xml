<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.rkb.dao.PublicModelDao">
    <select id="findPublicModel" resultType="PublicModel">
        select *
        from t_public_model
    </select>
    <select id="findPublicModelById" parameterType="Integer" resultType="PublicModel">
        select *
        from t_public_model
        where id = #{id}
    </select>
</mapper>